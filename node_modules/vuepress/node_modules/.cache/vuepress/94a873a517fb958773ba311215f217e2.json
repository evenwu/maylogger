nd-color: #fff;\n    opacity: .3;\n}\n/* mark */\n.tetris span.tetris-x {\n    background: black;\n    border-style: solid;\n    border-color: rgba(255,255,255,.1);\n    border-width: 1px;\n    background-color: #000;\n    position: relative;\n}\n.tetris-x:before,\n.tetris-x:after {\n    position: absolute;\n    left: calc(var(--tetris-size)/2 - 1px);\n    content: '';\n    height: var(--tetris-size);\n    width: 2px;\n    background: red;\n}\n.tetris-x:before {\n    transform: rotate(45deg);\n}\n.tetris-x:after {\n    transform: rotate(-45deg);\n}\n\n\n/* puyo */\n.puyo {\n    background: #666;\n    /* padding: calc(var(--tetris-size)/2); */\n    display: inline-block;\n    font-size: 0px;\n    margin: calc(var(--tetris-size)/2);\n    margin-left: 0;\n    position: relative;\n}\n.puyo span {\n    box-sizing: border-box;\n    display: inline-block;\n    width: var(--puyo-size);\n    height: var(--puyo-size);\n    border-radius: 50%;\n}\ndiv[class$=\"container\"] {\n    padding: calc(var(--tetris-size)/2);\n    filter: url(\"#puyo\");\n    position: absolute;\n    top: 0; left: 0;\n}\ndiv[class$=\"container\"]:first-child {\n    position: relative;\n}\n\n.puyo-r {\n    background: rgb(180,0,0);\n    box-shadow: -5px -5px 5px 0 rgba(0,0,0,1) inset,\n                2.5px 5px 5px 5px rgba(255,0,0,1) inset;\n\n}\n.puyo-g {\n    background: rgb(0,180,0);\n    box-shadow: -5px -5px 5px 0 rgba(0,0,0,1) inset,\n                2.5px 5px 5px 5px rgba(0,255,0,1) inset;\n}\n.puyo-b {\n    background: rgb(0,0,255);\n    box-shadow: -5px -5px 5px 0 rgba(0,0,0,1) inset,\n                2.5px 5px 5px 0px rgba(255,255,255,.3) inset;\n}\n.puyo-y {\n    background: rgb(180,180,0);\n    box-shadow: -5px -5px 5px 0 rgba(0,0,0,.5) inset,\n                2.5px 5px 5px 5px rgba(255,255,0,1) inset;\n}\n.puyo-p {\n    background: rgb(220,0,220);\n    box-shadow: -5px -5px 5px 0 rgba(0,0,0,1) inset,\n                2.5px 5px 5px 0px rgba(255,0,255,1) inset;\n}\n</style>"]}]}